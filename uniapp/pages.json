{  "lazyCodeLoading": "requiredComponents",
 "easycom": {
    // 是否开启自动扫描
    "autoscan": true,
    // 以正则方式自定义组件匹配规则
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      // 以 Xtx 开头的组件，在 components 文件夹中查找引入（需要重启服务器）
      "^Xtx(.*)": "@/components/Xtx$1.vue"
    }
  },
  "pages": [   
	  {
	    "path": "pages/CommList/CommList",  
	    "style": {  
	      "navigationBarTitleText": "社区",  
	      "enablePullDownRefresh": false  
	
	    }  
	  },  
  
    {  
      "path": "pages/FindList/FindList",  
      "style": {  
        "navigationBarTitleText": "发现",  
        "enablePullDownRefresh": false  
      }  
    },  
  
    {  
      "path": "pages/PageList/PageList",  
      "style": {  
        "navigationBarTitleText": "图鉴",  
        "enablePullDownRefresh": false  
      }  
    },
	{
	  "path": "pages/MessageList/MessageList",  
	  "style": {  
	    "navigationBarTitleText": "消息",  
	    "enablePullDownRefresh": false  
	  }  
	},
	{
	  "path": "pages/MyList/MyList",  
	  "style": {  
	    "navigationBarTitleText": "我的",  
	    "enablePullDownRefresh": false  
	  }  
	}
  ],  
  "globalStyle": {  
    "navigationBarTextStyle": "black",  
    "navigationBarBackgroundColor": "#F8F8F8",  
    "backgroundColor": "#F8F8F8",  
	"navigationStyle": "custom",
	

    "app-plus": {  
      "background": "#efeff4"  
    }  
  },  
  "tabBar": {  
    "color": "#999999",  
    "selectedColor": "#1AAD19", 
	 "borderStyle": "white",
    "list": [  
      
      {  
        "pagePath": "pages/CommList/CommList",  
        "text": "社区"
    
      },  
      {  
        "pagePath": "pages/FindList/FindList",  
        "text": "发现"
     
      },  
	  {
	    "pagePath": "pages/PageList/PageList",  
	    "text": "图鉴"
	     
	  }  ,
      {  
        "pagePath": "pages/MessageList/MessageList",  
        "text": "消息"
       
      },  
	  {
	    "pagePath": "pages/MyList/MyList",  
	    "text": "我的"
	  
	  }

    ]  
  }  
}